{
  "name": "portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "check-ts": "tsc --noEmit",
    "build:spa:nocheck": "cross-env NODE_ENV=production vite build",
    "build:spa": "npm run check-ts && npm run build:spa:nocheck",
    "build:ssr:nocheck": "cross-env BUILD_MODE=ssr NODE_ENV=production vite build --outDir=distSSR",
    "build:ssr": "npm run check-ts && npm run build:ssr:nocheck",
    "build:packages": "npm run check-ts && npm run build:ssr:nocheck && npm run build:spa:nocheck",
    "compress-artifacts": "node scripts/comp.js --formats=gzip",
    "copy-licenses": "node scripts/copyLicenses.js",
    "prerender:es": "node distSSR/ssr.es.js",
    "prerender": "node distSSR/ssr.cjs.js",
    "build": "npm run build:packages && npm run prerender && npm run copy-licenses && npm run compress-artifacts",
    "serve": "vite preview",
    "subset": "node subset.mjs",
    "font-comp": "node font"
  },
  "dependencies": {
    "@svelte-navigator/history": "^0.4.0",
    "classnames": "^2.3.1",
    "hydra-synth": "^1.3.9",
    "preact": "^10.5.15"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.5",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-url": "^6.1.0",
    "@squoosh/cli": "^0.7.2",
    "@types/css-declaration-sorter": "^6.0.1",
    "@types/html-minifier-terser": "^6.0.0",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^6.1.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-minifier-terser": "^6.0.2",
    "minimatch": "^3.0.4",
    "postcss": "^8.3.11",
    "preact-render-to-string": "^5.1.19",
    "preact-ssr-prepass": "^1.2.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.13",
    "wasm-brotli": "^2.0.2",
    "wasm-zopfli": "^1.0.2"
  }
}
